 DELETE  ACME.PROD.AR.ACCTFILE -
        CLUSTER -
        PURGE
 DEFINE CLUSTER -
        (NAME(ACME.PROD.AR.ACCTFILE) -
         VOLUMES(VOL001) -
         RECORDS(50000 10000) -
         RECORDSIZE(200 200) -
         KEYS(12 0) -
         INDEXED -
         SHAREOPTIONS(2 3)) -
        DATA -
        (NAME(ACME.PROD.AR.ACCTFILE.DATA) -
         CONTROLINTERVALSIZE(4096)) -
        INDEX -
        (NAME(ACME.PROD.AR.ACCTFILE.INDEX))
 REPRO  INFILE(BACKUPDD) -
        OUTDATASET(ACME.PROD.AR.ACCTFILE)
