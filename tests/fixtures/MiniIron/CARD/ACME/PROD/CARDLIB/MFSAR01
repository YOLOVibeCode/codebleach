         PRINT NOGEN
AR01MOD  FMT
         DEV TYPE=(3270,2),FEAT=(IGNORE),DSCA=X'0020'
         DIV TYPE=INOUT
         DPAGE CURSOR=((1,2))
         DFLD 'ACME INSURANCE',POS=(1,25),ATTR=(PROT,HI)
         DFLD 'AR INQUIRY',POS=(1,42),ATTR=(PROT,HI)
         DFLD 'ACCOUNT:',POS=(3,2),ATTR=(PROT,NORM)
AR01AIN  DFLD POS=(3,12),LTH=12,ATTR=(MOD,NUM)
         DFLD 'CLAIM:',POS=(3,28),ATTR=(PROT,NORM)
AR01CIN  DFLD POS=(3,36),LTH=15,ATTR=(MOD,NUM)
         DFLD 'PROVIDER:',POS=(5,2),ATTR=(PROT,NORM)
AR01PROV DFLD POS=(5,13),LTH=30,ATTR=(PROT,NORM)
         DFLD 'STATUS:',POS=(5,46),ATTR=(PROT,NORM)
AR01STAT DFLD POS=(5,55),LTH=10,ATTR=(PROT,NORM)
         DFLD POS=(24,2),LTH=79,ATTR=(PROT,NORM)
AR01MSG  DFLD POS=(24,2)
         FMTEND
AR01MI   MSG TYPE=INPUT,SOR=(AR01MOD,IGNORE)
         SEG
         MFLD AR01AIN,LTH=12
         MFLD AR01CIN,LTH=15
         MSGEND
AR01MO   MSG TYPE=OUTPUT,SOR=(AR01MOD)
         SEG
         MFLD (AR01PROV,','),LTH=30
         MFLD (AR01STAT,','),LTH=10
         MFLD (AR01MSG,','),LTH=79
         MSGEND
         END
